{
  "name": "airplane",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/airplane",
  "projectType": "application",
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "airplane dev"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/airplane/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "airplane deploy"
      }
    }
  },
  "tags": []
}
